<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="dubbo-service" owner="dubbo-service"/>
    <!-- 使用zookeeper注册中心暴露服务地址 -->
    <dubbo:registry address="zookeeper://106.13.215.47:2181" check="false" subscribe="false"/>
    <!-- 用户服务接口 -->
    <dubbo:service interface="com.anthony.service.demointerface.DemoInterface" ref="demoService"/>
<!--    <bean id="demoInterface" class="com.anthony.service.demoservice.DemoService"></bean>-->

</beans>